<body>
  <div class="container">
    <h1>COMPOSE</h1>
    <form action="/" method="post">
      <p><label for="TITLE">Title</label></p>
      <input type="text" id="titlebox" name="newblog" autocomplete="off" />

      <p><label for="POST">Post</label></p>
      <textarea id="postbox" rows="8" name="postblog"> </textarea>
      <br />
      <button type="submit" class="btn btn-primary">Publish</button>
    </form>
  </div>
</body>

